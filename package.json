{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "devback": "nodemon src/app.js",
    "dev": "nodemon src/app.js -d && next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "test:coverage": "npm test -- --coverage",
    "test:badges": "npm run test:coverage  && jest-coverage-badges"
  },
  "dependencies": {
    "express": "^4.17.1",
    "jest-coverage-badges": "^1.1.2",
    "next": "^10.0.0",
    "node-cron": "^3.0.0",
    "node-scrapy": "^0.5.0",
    "nodejs-base64-encode": "^1.1.0",
    "nodemon": "^2.0.7",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "save-dev": "0.0.1-security"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^26.0.23",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "axios": "^0.21.1",
    "babel-jest": "^27.0.2",
    "eslint": "^7.27.0",
    "eslint-plugin-react": "^7.24.0",
    "jest": "^27.0.3"
  },
  "jest": {
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ]
  }
}
